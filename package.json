{
  "name": "stripmq",
  "version": "0.0.6",
  "description": "Strip media queries from stylesheets",
  "bin": {
    "stripmq": "bin/stripmq"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "node bin/stripmq --input test/input.css --width 1000 --type screen --output /dev/null"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/jkenlooper/stripmq"
  },
  "keywords": [
    "mediaquery",
    "css"
  ],
  "main": "stripmq.js",
  "author": "Jake Hickenlooper <jake@weboftomorrow.com>",
  "contributors": [
    "Jorik Tangelder (http://joriktangelder.nl/)",
    "Tilo <tilomitra@gmail.com> (http://www.tilomitra.com/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkenlooper/stripmq/issues"
  },
  "homepage": "https://github.com/jkenlooper/stripmq",
  "dependencies": {
    "nopt": "^2.2.1",
    "css": "^1.6.0",
    "css-mediaquery": "^0.1.2"
  }
}
